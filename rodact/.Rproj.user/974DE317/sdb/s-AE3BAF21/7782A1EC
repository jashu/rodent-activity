n~Generation,se=F)+theme_bw()\nqplot(RearStart,RearDuration,data=subset(rearing,RearStart<=600),\n      group=acqClass,geom=\"smooth\",color=acqClass,facets=Zone~Test.Condition~.)+theme_bw()\nqplot(RearStart,RearDuration,data=subset(rearing,RearStart<=600 & !is.na(extClass)),\n      group=extClass,geom=\"smooth\",color=extClass,facets=Zone~Test.Condition~.)+theme_bw()\n\nqplot(Generation,maxRearDur,data=subset(RearData,Zone!=\"Doorway\"&Generation>2),\n      group=Line,geom=\"smooth\",method=\"lm\",color=Line,facets=Zone~Exposure)+theme_bw()\nqplot(Generation,meanRearDur,data=subset(RearData,Zone!=\"Doorway\"&Generation>2),\n      group=Line,geom=\"smooth\",method=\"lm\",color=Line,facets=Zone~Exposure)+theme_bw()\nqplot(Generation,varRearDur,data=subset(RearData,Zone!=\"Doorway\"&Generation>2),\n      group=Line,geom=\"smooth\",method=\"lm\",color=Line,facets=Zone~Exposure)+theme_bw()\nwrite.table(RearData,file=\"Activity/RearingDuration.txt\",sep=\"\\t\",row.names=FALSE)",
